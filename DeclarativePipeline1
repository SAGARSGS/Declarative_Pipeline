pipeline 
{
    agent any
    stages
    {
        stage('continuousdownload PB543')
        {
            steps
            {
                git 'https://github.com/krishnain/mavenab.git'
            }
        }
        stage('continuousbuild')
        {
            steps
            {
                sh 'mvn package'
            }
        }
     }
}
    
